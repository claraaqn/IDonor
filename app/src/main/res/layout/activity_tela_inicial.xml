<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F6FE"
    tools:context=".TelaInicial">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="681dp"
        android:layout_height="273dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/androidparty_png"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:id="@+id/ellipse_1"
        android:layout_width="412dp"
        android:layout_height="160dp"
        android:layout_marginTop="188dp"
        android:src="@drawable/fundo_elipse"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView" />

    <TextView
        android:id="@+id/d"
        android:layout_width="267dp"
        android:layout_height="102dp"
        android:fontFamily="@font/patuaone"
        android:gravity="center"
        android:text="@string/app_name"
        android:textColor="#023679"
        android:textSize="80sp"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_1"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/ellipse_1"
        app:layout_constraintVertical_bias="0.724" />


    <TextView
        android:id="@+id/textorededeapoio"
        android:layout_width="307dp"
        android:layout_height="78dp"
        android:fontFamily="@font/saira_medium"
        android:text="@string/texto_tela_inicial"
        android:textAlignment="center"
        android:textColor="#023679"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/d"
        app:layout_constraintVertical_bias="0.566" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="188dp"
        android:layout_height="29dp"
        android:fontFamily="@font/saira_semibold"
        android:gravity="center"
        android:text="@string/login_tela_inicial"
        app:layout_constraintBottom_toTopOf="@+id/buttonlogin"
        app:layout_constraintEnd_toEndOf="@+id/textorededeapoio"
        app:layout_constraintStart_toStartOf="@+id/textorededeapoio"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_1"
        app:layout_constraintVertical_bias="0.808" />

    <Button
        android:id="@+id/buttonlogin"
        android:layout_width="227dp"
        android:layout_height="48dp"
        android:layout_marginTop="172dp"
        android:backgroundTint="@color/AzulTitulo"
        android:onClick="LoginComEmail"
        android:text="@string/Login_com_email"
        android:textColor="@color/white"
        android:textColorHighlight="#023679"
        android:textColorLink="#023679"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textorededeapoio"
        app:layout_constraintStart_toStartOf="@+id/textorededeapoio"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_1"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/email2"
        android:layout_width="23dp"
        android:layout_height="20dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toBottomOf="@+id/buttonlogin"
        app:layout_constraintEnd_toEndOf="@+id/buttonlogin"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toStartOf="@+id/buttonlogin"
        app:layout_constraintTop_toTopOf="@+id/buttonlogin"
        app:layout_constraintVertical_bias="0.571"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/email1"
        android:layout_width="23dp"
        android:layout_height="20dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="11dp"
        android:layout_marginBottom="12dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toBottomOf="@+id/button2"
        app:layout_constraintEnd_toEndOf="@+id/button2"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toTopOf="@+id/button2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints"
        android:elevation="8dp"/>


    <TextView
        android:id="@+id/OU1"
        android:layout_width="73dp"
        android:layout_height="24dp"
        android:layout_marginBottom="120dp"
        android:fontFamily="@font/saira_semibold"
        android:text="@string/OU"
        android:textAlignment="center"
        android:textColor="#023679"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonlogin"
        app:layout_constraintVertical_bias="0.851" />

    <Button
        android:id="@+id/button2"
        android:layout_width="227dp"
        android:layout_height="43dp"
        android:backgroundTint="@color/BackBotao"
        android:onClick="CadastrarComEmail"
        android:text="@string/Cadastrar_com_email"
        android:textColor="@color/AzulTitulo"
        android:textColorHighlight="#023679"
        android:textColorLink="#023679"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonlogin"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/buttonlogin"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        app:layout_constraintVertical_bias="0.147"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="121dp"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        android:fontFamily="@font/saira_semibold"
        android:gravity="center"
        android:text="@string/faca_cadastro"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/buttonlogin"
        app:layout_constraintStart_toStartOf="@+id/buttonlogin"
        app:layout_constraintTop_toBottomOf="@+id/buttonlogin"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/conhecer_as"
        android:layout_width="217dp"
        android:layout_height="56dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@android:color/transparent"
        android:gravity="center"
        android:text="@string/Conhecer_as_instituições"
        android:textColor="#023679"
        android:textColorHighlight="#023679"
        android:textColorLink="#023679"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button2"
        app:layout_constraintStart_toStartOf="@+id/button2"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.723" />


</androidx.constraintlayout.widget.ConstraintLayout>