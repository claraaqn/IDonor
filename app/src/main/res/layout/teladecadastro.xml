<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F6FE"
    tools:context=".TelaCadastro">

    <EditText
        android:id="@+id/senha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="104dp"
        android:ems="10"
        android:inputType="textPassword"
        android:text="@string/login_senha"
        android:textColor="#979494"
        app:layout_constraintBottom_toTopOf="@+id/linha6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lock"
        app:layout_constraintTop_toBottomOf="@+id/telefone"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/nome_comple6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="116dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:text="@string/EndereÃ§o_"
        android:textColor="#979494"
        app:layout_constraintBottom_toTopOf="@+id/linha7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/home_page"
        app:layout_constraintTop_toBottomOf="@+id/linha6"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/telefone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="105dp"
        android:ems="10"
        android:inputType="phone"
        android:text="@string/Telefone_"
        android:textColor="#979494"
        app:layout_constraintBottom_toTopOf="@+id/linha5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/phone_messa"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple3"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/nome_comple3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="59dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="3dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="E_mail"
        android:textColor="#979494"
        app:layout_constraintBottom_toTopOf="@+id/telefone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/tear_off_ca"
        app:layout_constraintTop_toBottomOf="@+id/linha3"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/nome_comple2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="60dp"
        android:ems="10"
        android:inputType="text|date"
        android:text="@string/data_de_nascimento"
        android:textColor="#979494"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple3"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_1"
        app:layout_constraintStart_toStartOf="@+id/tear_off_ca"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <View
        android:id="@+id/linha7"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="179dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple6"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha6"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="4dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/nome_comple6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/senha"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha5"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="6dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/senha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/telefone"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha3"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="9dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/nome_comple3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple2"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha4"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="8dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/telefone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple3"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imagem"
        android:layout_width="975dp"
        android:layout_height="293dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toTopOf="@+id/ellipse_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/androidparty2_png"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/ellipse_1"
        android:layout_width="413dp"
        android:layout_height="102dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="47dp"
        android:background="#F4F6FE"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_2"
        tools:ignore="MissingConstraints"
        tools:layout_constraintCircleRadius="10dp" />


    <View
        android:id="@+id/ellipse_2"
        android:layout_width="421dp"
        android:layout_height="136dp"
        android:layout_marginTop="194dp"
        android:layout_marginBottom="12dp"
        android:background="#F4F6FE"
        app:layout_constraintBottom_toBottomOf="@+id/cadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imagem"
        app:layout_constraintVertical_bias="0.0"
        tools:background="@drawable/ellipse_2"
        tools:ignore="MissingConstraints"
        tools:layout_constraintCircleRadius="10dp" />

    <TextView
        android:id="@+id/cadastro"
        android:layout_width="241dp"
        android:layout_height="117dp"
        android:layout_marginStart="90dp"
        android:layout_marginTop="225dp"
        android:layout_marginEnd="90dp"
        android:fontFamily="@font/patuaone"
        android:gravity="center_horizontal|center_vertical"
        android:text="Cadastro"
        android:textColor="#023679"
        android:textSize="44sp"
        app:layout_constraintBottom_toBottomOf="@+id/imagem"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_2"
        app:layout_constraintStart_toStartOf="@+id/ellipse_2"
        app:layout_constraintTop_toTopOf="@+id/imagem"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha2"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="5dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toTopOf="@+id/nome_comple2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_1"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/user"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toTopOf="@+id/linha2"
        app:layout_constraintEnd_toStartOf="@+id/nome_comple"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_1"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nome_comple"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="96dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="108dp"
        android:layout_marginBottom="52dp"
        android:ems="10"
        android:inputType="text"
        android:text="@string/nome_completo"
        android:textColor="#979494"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple2"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/ellipse_1"
        app:layout_constraintTop_toBottomOf="@+id/ellipse_2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <View
        android:id="@+id/tear_off_ca"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="11dp"
        android:background="@drawable/tear_off_calendar"
        app:layout_constraintBottom_toTopOf="@+id/linha3"
        app:layout_constraintEnd_toStartOf="@+id/nome_comple2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linha2"
        tools:ignore="MissingConstraints" />


    <View
        android:id="@+id/email"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toTopOf="@+id/linha4"
        app:layout_constraintEnd_toStartOf="@+id/nome_comple3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linha3"
        tools:ignore="MissingConstraints" />


    <View
        android:id="@+id/phone_messa"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="15dp"
        android:background="@drawable/phone_message"
        app:layout_constraintBottom_toTopOf="@+id/linha5"
        app:layout_constraintEnd_toStartOf="@+id/senha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linha4"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/lock"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="17dp"
        android:background="@drawable/lock"
        app:layout_constraintBottom_toTopOf="@+id/linha6"
        app:layout_constraintEnd_toEndOf="@+id/senha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linha5"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/home_page"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="238dp"
        android:layout_marginBottom="9dp"
        android:background="@drawable/home_page"
        app:layout_constraintBottom_toTopOf="@+id/linha7"
        app:layout_constraintEnd_toEndOf="@+id/senha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linha6"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="155dp"
        android:layout_height="40dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="228dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="48dp"
        android:text="@string/Cadastrar_"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple6"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />


    <View
        android:id="@+id/voltar"
        android:layout_width="19dp"
        android:layout_height="17dp"
        android:layout_marginStart="196dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="196dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>