<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4F6FE"
    tools:context=".TelaCadastro">

    <ImageView
        android:id="@+id/imagem"
        android:layout_width="411dp"
        android:layout_height="273dp"
        android:layout_marginTop="-20dp"
        app:layout_constraintBottom_toTopOf="@+id/nome_comple"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/androidparty2_png"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ellipse_2"
        android:layout_width="412dp"
        android:layout_height="160dp"
        android:src="@drawable/fundo_elipse"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239" />

    <TextView
        android:id="@+id/cadastro"
        android:layout_width="241dp"
        android:layout_height="117dp"
        android:fontFamily="@font/patuaone"
        android:gravity="center"
        android:text="@string/cadastro_cadastro"
        android:textColor="#023679"
        android:textSize="44sp"
        app:layout_constraintBottom_toBottomOf="@+id/ellipse_2"
        app:layout_constraintEnd_toEndOf="@+id/ellipse_2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/ellipse_2"
        app:layout_constraintTop_toTopOf="@+id/ellipse_2"
        app:layout_constraintVertical_bias="0.511" />

    <EditText
        android:id="@+id/nome_comple"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:hint="@string/nome_completo"
        android:inputType="text"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/cadastro"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/cadastro"
        app:layout_constraintTop_toBottomOf="@+id/cadastro" />

    <View
        android:id="@+id/home_page"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/home_page"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple6"
        app:layout_constraintEnd_toEndOf="@+id/lock"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/lock"
        app:layout_constraintTop_toTopOf="@+id/nome_comple6"
        app:layout_constraintVertical_bias="0.611"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha3"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple2"
        app:layout_constraintVertical_bias="0.841"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha7"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple6"
        app:layout_constraintVertical_bias="0.841"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/calendario"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/tear_off_calendar"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple2"
        app:layout_constraintEnd_toEndOf="@+id/user"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/user"
        app:layout_constraintTop_toTopOf="@+id/nome_comple2"
        app:layout_constraintVertical_bias="0.777" />

    <View
        android:id="@+id/lock"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/lock"
        app:layout_constraintBottom_toBottomOf="@+id/senha"
        app:layout_constraintEnd_toEndOf="@+id/phone_messa"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/phone_messa"
        app:layout_constraintTop_toTopOf="@+id/senha"
        app:layout_constraintVertical_bias="0.611"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nome_comple6"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="@string/EndereÃ§o_"
        android:inputType="text|textPostalAddress"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/senha"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/senha"
        app:layout_constraintTop_toBottomOf="@+id/senha" />

    <View
        android:id="@+id/phone_messa"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@drawable/phone_message"
        app:layout_constraintBottom_toBottomOf="@+id/telefone"
        app:layout_constraintEnd_toEndOf="@+id/email"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/email"
        app:layout_constraintTop_toTopOf="@+id/telefone"
        app:layout_constraintVertical_bias="0.611" />

    <EditText
        android:id="@+id/senha"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="@string/login_senha"
        android:inputType="textPassword"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/telefone"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/telefone"
        app:layout_constraintTop_toBottomOf="@+id/telefone" />

    <View
        android:id="@+id/linha6"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/senha"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/senha"
        app:layout_constraintVertical_bias="0.841"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/email"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/email"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple3"
        app:layout_constraintEnd_toEndOf="@+id/calendario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/calendario"
        app:layout_constraintTop_toTopOf="@+id/nome_comple3"
        app:layout_constraintVertical_bias="0.666"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/user"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/user"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple"
        app:layout_constraintEnd_toStartOf="@+id/nome_comple"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple"
        app:layout_constraintVertical_bias="0.611"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/linha4"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="8dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple3"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/nome_comple3"
        android:layout_width="212dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="@string/login_email"
        android:inputType="textEmailAddress"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/nome_comple2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nome_comple2"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple2" />

    <EditText
        android:id="@+id/nome_comple2"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="@string/data_de_nascimento"
        android:inputType="date|datetime"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/nome_comple"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nome_comple"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/telefone"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="@string/Telefone_"
        android:inputType="number|phone"
        android:textColor="#979494"
        app:layout_constraintEnd_toEndOf="@+id/nome_comple3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nome_comple3"
        app:layout_constraintTop_toBottomOf="@+id/nome_comple3" />

    <View
        android:id="@+id/linha5"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/telefone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/telefone"
        app:layout_constraintVertical_bias="0.85" />

    <View
        android:id="@+id/linha2"
        android:layout_width="364dp"
        android:layout_height="2dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:background="@android:drawable/dark_header"
        app:layout_constraintBottom_toBottomOf="@+id/nome_comple"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nome_comple"
        app:layout_constraintVertical_bias="0.869"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/button"
        android:layout_width="155dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/AzulTitulo"
        android:text="@string/Cadastrar_"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/cadastro"
        app:layout_constraintStart_toStartOf="@+id/cadastro"
        app:layout_constraintTop_toBottomOf="@+id/linha7"
        app:layout_constraintVertical_bias="0.257" />

    <View
        android:id="@+id/voltar"
        android:layout_width="19dp"
        android:layout_height="17dp"
        android:background="@drawable/voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button"
        app:layout_constraintStart_toStartOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/button"
        app:layout_constraintVertical_bias="0.221"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>